<UserControl x:Class="IntoApp.UseControl.Empty"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:IntoApp.UseControl"
             xmlns:Controls="clr-namespace:Skin.WPF.Controls;assembly=Skin.WPF"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <WrapPanel HorizontalAlignment="Center" VerticalAlignment="Center"
                   Margin="5">
            <Controls:UseImage Image="{StaticResource Icon_Black}"
                               ImageBrush="#C2C3C9"
                               ImageWidth="100" ImageHeight="90"/>
        </WrapPanel>
    </Grid>
</UserControl>
